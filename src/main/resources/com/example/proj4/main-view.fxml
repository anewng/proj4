<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="500.0" maxWidth="500.0" minHeight="500.0" minWidth="500.0" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proj4.MainController">
   <children>
      <GridPane prefHeight="500.0" prefWidth="500.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
      <ImageView fitHeight="500.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@/backgroundMainMenuImage.png" />
         </image></ImageView>
      <Button fx:id="donutButton" layoutX="69.0" layoutY="156.0" mnemonicParsing="false" onAction="#onDonutButtonClick" prefHeight="155.0" prefWidth="163.0" style="-fx-background-color: transparent;" />
      <Button fx:id="coffeeButton" layoutX="272.0" layoutY="156.0" mnemonicParsing="false" onAction="#onCoffeeButtonClick" prefHeight="155.0" prefWidth="163.0" style="-fx-background-color: transparent;" />
      <Button fx:id="yourOrderButton" layoutX="118.0" layoutY="355.0" mnemonicParsing="false" onAction="#onYourOrderButtonClick" prefHeight="118.0" prefWidth="120.0" style="-fx-background-color: transparent;" />
      <Button fx:id="storeOrderButton" layoutX="272.0" layoutY="355.0" mnemonicParsing="false" onAction="#onStoreOrderButtonClick" prefHeight="118.0" prefWidth="120.0" style="-fx-background-color: transparent;" />
   </children>
</AnchorPane>
