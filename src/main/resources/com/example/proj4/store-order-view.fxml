<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="400" maxWidth="400" minHeight="400" minWidth="400" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proj4.StoreOrderViewController">
    <children>
        <ImageView fitHeight="400.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@/storeorderbackground.png" />
            </image></ImageView>
      <ComboBox fx:id="orderNumber" layoutX="309.0" layoutY="22.0" onAction="#onOrderNumberSelected" prefHeight="25.0" prefWidth="34.0" />
      <ListView fx:id="storeOrders" layoutX="42.0" layoutY="102.0" prefHeight="230.0" prefWidth="313.0" />
      <Button fx:id="cancelOrder" layoutX="60.0" layoutY="351.0" mnemonicParsing="false" onAction="#onCancelOrderButtonClick" prefHeight="25.0" prefWidth="97.0" style="-fx-background-color: transparent;" />
      <Button fx:id="exportOrder" layoutX="236.0" layoutY="351.0" mnemonicParsing="false" onAction="#onExportOrderButtonClick" prefHeight="25.0" prefWidth="97.0" style="-fx-background-color: transparent;" />
      <TextField fx:id="total" layoutX="309.0" layoutY="59.0" prefHeight="25.0" prefWidth="74.0" />
    </children>
</AnchorPane>
